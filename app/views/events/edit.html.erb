<%= form_for @event, html: { multipart: true }, :url => event_path(@event), :method => :patch do |f| %>

    <%= render :partial => "form", :locals => { :f => f } %>
    
    <%= f.submit "Update", :class => "btn btn-primary" %>
    <%= link_to 'Cancel', events_path, :class => "btn btn-primary" %>
    
<% end %>
