	<%= form_for @event, html: { multipart: true }, :url => event_path(@event), :method => :patch do |f| %>

	    <%= render :partial => "form", :locals => { :f => f } %>
	    
	    <%= f.submit "更新", :class => "btn btn-primary" %>
	    <%= link_to '返回', events_path, :class => "btn btn-primary" %>
	    
	<% end %>
</div>